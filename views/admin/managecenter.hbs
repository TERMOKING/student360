
<div class="main">
  <div class="head-bar">
    <h2 class="card_headding">Add Center</h2>
  </div>
  <form class="row g-3" action="/managecenter" method="POST" onsubmit="return submitForm(this);"> 
    



      <h5 class="card_subheadding">Personal Details :</h5>

      <div class="col-md-6">
        <label for="fname" class="form-label">Center Name</label>
        <input type="text" class="form-control" name="centername" required>
      </div>
      <div class="col-md-6">
        <label for="lname" class="form-label">Center id</label>
        <input type="text" class="form-control" placeholder="eg : nec , ttc" name="centerid" required>
      </div>

      <div class="col-md-6">
        <label for="email" class="form-label">Address</label>
        <input type="address" class="form-control" name="address"  required>
      </div>

    <div class="buttons-grp">
      <button type="reset" class="btn btn-danger">  Reset  </button>
      <button  class="btn btn-success" type="submit" value="Submit">Submit</button>
    </div>
    </div>
    

  </form>
</div>




<script>
 function submitForm()
{
 
    Swal.fire({
  title: 'Updated',
  icon: 'success',
  timer: 4000,
   showCancelButton: false,
  showConfirmButton: false

}).then((result) => {
  if (result.isConfirmed) {
      window.location='/dashboard'
  }
})

}

</script>



