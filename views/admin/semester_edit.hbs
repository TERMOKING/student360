

<div class="profile_page">

    <div class="col-md-12">
    <div class="head-bar">
    <h2 class="card_headding">Semester Update</h2>
  </div><br>
            <div class="col-lg-12" style="margin-bottom: 53px;">
        <form method="post">
                <br>
                <label style="profile-caps"class="form-label" for="role" style="margin-right:5px;">Course : </label>
                <select  class="form-select" name="courseid" required>
                    <option value="">Choose..</option>
                    {{#each course}}
                    <option value="{{this.courseid}}">{{this.coursename}}</option>
                    {{/each}}
                </select>
            </div>
            <button type="submit" onclick="submitForm()" class="excel_submit_button"> submit</button>   
        </form>
    </div>
</div>

<script>
 function submitForm()
{
    Swal.fire({
  title: 'Updated',
  icon: 'success',
  timer: 4000,
   showCancelButton: false,
  showConfirmButton: false

}).then((result) => {
  if (result.isConfirmed) {
      window.location='/semester_edit'
  }
})

}

</script>


